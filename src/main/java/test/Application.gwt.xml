<module rename-to="testeegree">
    <inherits name="com.google.gwt.logging.Logging"/>
    <!--    <set-property name="gwt.logging.logLevel"   
    value="INFO" />            -->
    <set-property name="gwt.logging.enabled"   
                  value="TRUE" />   
    <set-property name="gwt.logging.developmentModeHandler"   
                  value="ENABLED" />    
    <set-property name="gwt.logging.systemHandler"   
                  value="ENABLED" />  
    <set-property name="gwt.logging.popupHandler"   
                  value="DISABLED" />  
    <set-property name="gwt.logging.consoleHandler"   
                  value="ENABLED"/>   
    <set-property name="gwt.logging.firebugHandler"   
                            value="ENABLED" />  
    
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.user.theme.standard.Standard" />
    <inherits name="org.geomajas.puregwt.GeomajasClientImpl" />
    <inherits name="org.geomajas.puregwt.theme.GeomajasDefaultTheme" />
    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
    
    <!-- add required languages
    <extend-property name="locale" values="nl"/>
    -->

    <!-- set target browser to compile for, use this to limit to the browser used for testing -->
    <!-- where value = "ie6/opera/gecko1_8/safari/gecko" , "gecko1_8" is FireFox 3 -->
    <!--
    -->
    <set-property name="user.agent" value="gecko1_8" />

    <entry-point class="test.client.Application"/>
</module>